<table class="table">
	<thead>
		<tr>
			<th>ID</th>
			<th>Card Id</th>
			<th>Type</th>
			<th>Amount</th>
			<th>New Balance</th>
			<th>Time</th>
			<th>TerminalId</th>
			<th>Payment Application</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody>
		<tr ng-repeat="transaction in colectionTransaction">
			<td>
				{{transaction.id}}
			</td>
			<td>
				{{transaction.cardId}}
			</td>
			<td>
				{{transaction.type}}
			</td>
			<td>
				{{transaction.amount}}
			</td>
			<td>
				{{transaction.newBalance}}
			</td>
			<td>
				{{transaction.time | date:'hh:mm:ss a dd/MMM/yy'}}
			</td>
			<td>
				{{transaction.terminalId}}
			</td>
			<td>
				{{transaction.paymentApplicationId}}
			</td>
			<td>
				{{transaction.description}}
			</td>

		</tr>
	</tbody>
</table>