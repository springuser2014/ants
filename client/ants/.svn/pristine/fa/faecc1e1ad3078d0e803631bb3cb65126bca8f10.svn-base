<div class="page-breadcrumbs">
    <ul class="breadcrumb" style="margin-right: 0px;">
        <li>
            <i class="fa fa-home"></i>
        </li>
    </ul>
    <ul class="breadcrumb" >
        <li ng-hide="item.url==''" class="active" ng-repeat="item in initValue.variablePath">
            <a  href="{{item.url}}">{{item.value}}</a>
        </li>
        <li ng-show="item.url==''" class="active" ng-repeat="item in initValue.variablePath">
            {{item.value}}
        </li>
    </ul>
</div>

<div class="page-header position-relative">
    <div class="header-title">
        <h1>
            {{initValue.title}}
        </h1>
    </div>
    <div class="header-buttons">
        <a class="sidebar-toggler" href="#">
            <i class="fa fa-bars"></i>
        </a>
        <a class="fullscreen" id="fullscreen-toggler" href="#">
            <i class="glyphicon glyphicon-fullscreen"></i>
        </a>
    </div>
</div>


<div class="page-body">
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-xs-12" ng-controller='ColectionTransactionController'>
            <div class="widget-box">
                <div class="widget-header">
                    <h5 class="widget-caption">All Transaction in System</h5>
                </div><!--Widget Header-->
                <div class="widget-body">
                    <div class="widget-main">
                        <transaction-table></transaction-table>
                    </div><!--Widget Main Container-->
                </div><!--Widget Body-->
            </div>
        </div>
    </div>
</div>


<script type="text/javascript">
    fullcreenHandler()
    handleSideMenu()
    initiateWidgetBoxes()
    $("#sidebar-collapse .collapse-icon").click()
</script>