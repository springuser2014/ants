(function(angular) {
	var transactionDirective = angular.module('transaction-directive', [])
	.directive('transactionTable', function() {
		return {
			restrict : 'AEC',
			templateUrl : config.directive.transaction.table
		}
	})
	.directive('filterTransaction', function() {
		return {
			restrict : 'AEC',
			templateUrl : config.directive.transaction.filter
		}
	})
	.directive('pagingTransaction', function(){
		return {
			restrict : 'AEC',
			templateUrl : config.directive.transaction.paging
		}
	})
})(window.angular);